                    "\r",
                    "// add and activate tab panel\r",
                    "if(main && main.layout) {\r",
                    "    // Search for existing tab\r",
                    "    Ext.each(main.layout.getLayoutItems(), function(panel, index) {\r",
                    "        if(panel.getItemId() == panelId) {\r",
                    "            tabFound = true;\r",
                    "        }\r",
                    "    });\r",
                    "    \r",
                    "    // add new tab if doesn't already exist\r",
                    "    if(!tabFound) {\r",
                    "        var classname = panelId.charAt(0).toUpperCase() + panelId.slice(1);\r",
                    "        var newpanelclass = 'sxapim.view.'+classname;\r",
                    "        var newpanel = Ext.create({\r",
                    "         xclass: newpanelclass\r",
                    "        });\r",
                    "        main.add(newpanel);\r",
                    "    }\r",
                    "    // select appropriate tab\r",
                    "    main.layout.setActiveItem(panelId);\r",
                    "}\r",
                    "\r",
                    "// Set menu and page title\r",
                    "if(menu && menu.items) {\r",
                    "    menu.items.each(function(item) {\r",
                    "        // Active\r",
